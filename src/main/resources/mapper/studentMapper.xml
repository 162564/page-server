<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.pageserver.mapper.StudentMapper">
    <select id="selectBySid" resultType="student" parameterType="int">
        select * from student where sid = #{sid}
    </select>
    <select id="checkInfo" resultType="student" parameterType="string">
        select * from student where username = #{username} and password = #{password}
    </select>
</mapper>